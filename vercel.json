{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
  "routes": [
    {
      "src": "/api/swagger-spec.json",
      "dest": "public/swagger-spec.json"
    },
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ]
}